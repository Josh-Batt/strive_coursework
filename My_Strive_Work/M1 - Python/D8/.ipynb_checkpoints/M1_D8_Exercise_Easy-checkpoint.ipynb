   "source": [
    "page = requests.get(\"https://forecast.weather.gov/MapClick.php?lat=37.777120000000025&lon=-122.41963999999996\")\n",
    "soup = BeautifulSoup(page.content, 'html.parser')\n",
    "\n",
    "#target = soup.find_all(id=\"seven-day-forecast-container\") # Creats ruleset, can't manipulate further\n",
    "target = soup.find_all(\"div\", {\"id\": \"seven-day-forecast-container\"})[0] # Not 100% correct info, do easy then do hard\n",
    "\n",
    "\n",
    "all_p = target.find_all(\"p\")\n",
    "#all_p = target.find_all(text=True) # Strips off short weather descriptions\n",
    "\n",
    "#clean_p = all_p.get_text() # all_p is another ruleset, can't be manipulated like this\n",
    "\n",
    "all_p_list = []\n",
    "\n",
    "for x in all_p:\n",
    "    all_p_list.append(x.get_text()) # Strips off weather descriptions # Once a list you can clean it more\n",
    "    \n",
    "all_p_list\n",
    "\n",
    "all_p_list = [all_p_list[i:i + 10] for i in range(0, len(all_p_list))]\n",
    "all_p_list"
   ]
